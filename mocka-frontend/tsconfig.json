{
  "include": ["src", "typings"],
  "compilerOptions": {
    "baseUrl": "src",
    "paths": {
      "mocka/config": ["config.ts"],
      "mocka/constants/*": ["constants/*"],
      "mocka/api/*": ["api/*"]
    },
    "module": "CommonJS",
    "moduleResolution": "node",
    "noEmit": true,
    "checkJs": true,
    "jsx": "preserve",
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true
  }
}
